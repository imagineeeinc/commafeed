{
    "$schema": "https://json.schemastore.org/web-manifest-combined.json",
    "name": "CommaFeed",
    "theme_color": "#f88a14",
    "background_color": "#f88a14",
    "short_name": "CommaFeed",
    "id": "com.athou.commafeed",
    "description": "Google Reader inspired self-hosted RSS reader.",
    "orientation": "any",
    "display": "standalone",
    "scope": ".",
    "start_url": "/",
    "share_target": {
        "action": "/rest/feed/subscribe",
        "method": "GET",
        "params": {
            "url": "url"
        }
    },
    "shortcuts": [
        {
            "name": "Open newest unread",
            "url": "/next?category=all&order=desc",
            "description": "Open newest unread"
        },
        {
            "name": "Open oldest unread",
            "url": "/next?category=all&order=asc",
            "description": "Open oldest unread"
        }
    ],
    "icons": [
        {
            "src": "app-icon-72.png",
            "sizes": "72x72",
            "type": "image/png",
            "density": "1.5"
        },
        {
            "src": "app-icon-114.png",
            "sizes": "96x96",
            "type": "image/png",
            "density": "2.0"
        },
        {
            "src": "app-icon-144.png",
            "sizes": "144x144",
            "type": "image/png",
            "density": "3.0"
        },
        {
            "src": "app-icon-192.png",
            "sizes": "192x192",
            "type": "image/png",
            "density": "4.0"
        }
    ]
}
